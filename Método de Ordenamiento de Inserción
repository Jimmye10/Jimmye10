# Atravesar 1 hasta len(arr)
    for i in range(1, len(arr)):
        key = arr[i]
 
        # Mover elementos de arr[0..i-1], que son mayores que key, a una posiciÃ³n por delante de su posiciÃ³n actual
        j = i - 1
        while j >= 0 and arr[j] > key:
            arr[j + 1] = arr[j]
            j -= 1
        arr[j + 1] = key
